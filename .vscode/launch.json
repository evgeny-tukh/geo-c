    "configurations": [
        {
            "name": "calc RL range&bearing",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/gt.exe",
            "args": [
                "b",
                "-o:59,29",
                "-d:60,30",
                "-m:o"
